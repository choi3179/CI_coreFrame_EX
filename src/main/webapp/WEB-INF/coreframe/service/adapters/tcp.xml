<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context"
  xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans   
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-2.5.xsd">


  <bean id="adapter.tcp" class="corelogic.adapter.io.TcpResourceAdapter">
    <property name="name" value="tcp" />

    <property name="connectionFactoryClassName" value="corelogic.adapter.io.TcpConnectionFactory" />
    <property name="processorFactoryClassName" value="corelogic.adapter.io.TcpProcessorFactory" />
    <property name="processorClassName" value="corelogic.adapter.io.TcpProcessor" />



    <property name="managementService" ref="corelogic.adapterService" />
  </bean>


</beans>